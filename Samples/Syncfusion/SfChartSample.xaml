<UserControl x:Class="MGK_Analyzer.Samples.Syncfusion.SfChartSample"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
             Height="300" Width="600">
    <Grid>
        <syncfusion:SfChart x:Name="SampleChart">
            <syncfusion:SfChart.PrimaryAxis>
                <syncfusion:DateTimeAxis IntervalType="Days" />
            </syncfusion:SfChart.PrimaryAxis>
            <syncfusion:SfChart.SecondaryAxis>
                <syncfusion:NumericalAxis />
            </syncfusion:SfChart.SecondaryAxis>

            <syncfusion:LineSeries ItemsSource="{Binding SeriesData}" XBindingPath="Time" YBindingPath="Value"/>
        </syncfusion:SfChart>
    </Grid>
</UserControl>